<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <!--  定义全局颜色  -->
  <Color x:Key="OK_COLOR">#FF4CAF50</Color>
  <SolidColorBrush x:Key="OK_COLOR_Brush" Color="{StaticResource OK_COLOR}" />
  <Color x:Key="Cancer_COLOR">#B61616</Color>
  <SolidColorBrush x:Key="Cancer_COLOR_Brush" Color="{StaticResource Cancer_COLOR}" />
</ResourceDictionary>